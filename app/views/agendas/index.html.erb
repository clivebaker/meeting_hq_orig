
<h1>Agendas</h1>

<div id="agendas">
  <% @agendas.each do |agenda| %>
    <%= render agenda %>
    <p>
      <%= link_to "Show this agenda", organisation_meeting_agenda_path(@organisation, @meeting, agenda) %>
    </p>
  <% end %>
</div>

<%= link_to "New agenda", new_organisation_meeting_agenda_path(@organisation, @meeting) %>
