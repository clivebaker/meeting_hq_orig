<div id="<%= dom_id meeting_action %>">
  <p>
    <strong>Meeting:</strong>
    <%= meeting_action.meeting_name %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= meeting_action.name %>
  </p>

  <p>
    <strong>Note:</strong>
    <%= meeting_action.note %>
  </p>

  <p>
    <strong>Position:</strong>
    <%= meeting_action.position %>
  </p>

  <p>
    <strong>State:</strong>
    <%= meeting_action.aasm_state %>
  </p>

  <p>
    <strong>User:</strong>
    <%= meeting_action.user_name %> (<%= meeting_action.user_initials %>)
  </p>

</div>
