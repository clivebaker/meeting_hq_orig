<p style="color: green"><%= notice %></p>

<h1>Meetings</h1>

<div id="meetings">
  <% @meetings.each do |meeting| %>
    <%= render meeting %>
    <p>
      <%= link_to "Show this meeting", organisation_meeting_path(@organisation, meeting) %>
    </p>
  <% end %>
</div>

<%= link_to "New meeting", new_organisation_meeting_path(@organisation) %>
